{
  "chainer":{
    "in": ["x"],
    "pipe": [
      {
        "name": "str_lower",
        "id": "lower",
        "in": ["x"],
        "out": ["x_lower"]
      },
      {
        "name": "nltk_moses_tokenizer",
        "id": "tokenizer",
        "in": ["x_lower"],
        "out": ["x_tokens"]
      },
      {
        "id": "vocab",
        "name": "simple_vocab",
        "save_path": "vocabs/russian_words_vocab.dict",
        "load_path": "vocabs/russian_words_vocab.dict"
      },
      {
        "in": ["x_tokens"],
        "out": ["tokens_candidates"],
        "class": "deeppavlov.models.spelling_correction.levenshtein.searcher_component:LevenshteinSearcherComponent",
        "words": "#vocab.keys()"
      },
      {
        "class": "deeppavlov.models.spelling_correction.electors.kenlm_elector:KenlmElector",
        "in": ["tokens_candidates"],
        "out": ["y_predicted_tokens"],
        "load_path": "language_models/ru_wiyalen_no_punkt.arpa.binary"
      },
      {
        "ref": "tokenizer",
        "in": ["y_predicted_tokens"],
        "out": ["y_predicted"]
      }
    ],
    "out": ["y_predicted"]
  },
  "metadata": {
    "requirements": [
      "../dp_requirements/spelling.txt"
    ],
    "labels": {
      "telegram_utils": "ErrorModel",
      "server_utils": "ErrorModel"
    },
    "download": [
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/vocabs/russian_words_vocab.dict.gz",
        "subdir": "vocabs"
      },
      {
        "url": "http://files.deeppavlov.ai/lang_models/ru_wiyalen_no_punkt.arpa.binary.gz",
        "subdir": "language_models"
      }
    ]
  }
}
